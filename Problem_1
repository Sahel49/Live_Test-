class Student {
  String name;
  int roll;

  // Constructor
  Student(this.name, this.roll);

  // Method to display info
  void displayInfo() {
    print("Student Name: $name");
    print("Roll Number: $roll");
  }
}

void main() {
  // Create one student object
  Student s1 = Student("Sahel", 18);
  s1.displayInfo();
}
